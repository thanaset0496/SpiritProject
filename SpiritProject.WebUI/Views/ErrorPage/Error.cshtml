@{
    Layout = "~/Views/Shared/_Layout_Error.cshtml";
    ViewBag.Title = "Error";
}

<div class="error-container">
    <i class="pe-7s-way text-success big-icon"></i>
    <h1>Error</h1>
    <strong>Page Unavailable</strong>
    <p>
        Sorry, but the page you are looking for has note been unavailable. Try checking the URL for error, then hit the refresh button on your browser or try found something else in our app.

    </p>
    <a onclick="window.history.go(-1);" class="btn btn-xs btn-success">Back</a>

</div>
